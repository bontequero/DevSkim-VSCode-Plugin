{
  "name": "devskim-analyzer",
  "description": "Devskim Security Analyzer",
  "version": "0.2.1",
  "author": "Microsoft Corporation",
  "license": "MIT",
  "main": "cli.js",
  "preferGlobal": true,
  "bin": {
    "devskim": "cli.js"
  },
  "engines": {
    "node": "*"
  },
  "private": true,
  "dependencies": {
    "commander": "^2.11.0",
    "mkdirp": "^0.5.1",
    "node-dir": "^0.1.17",
    "safe-regex": "^1.1.0",
    "vscode-languageserver": "^3.4.3",
    "xregexp": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.31",
    "typescript": "^2.5.3"
  },
  "scripts": {
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p ."
  }
}
